{
  "name": "stencil-initialization-bug",
  "version": "0.0.1",
  "description": "Stencil Component Starter",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/stencil-initialization-bug/stencil-initialization-bug.esm.js",
  "exports": {
    ".": {
      "import": "./dist/stencil-initialization-bug/stencil-initialization-bug.esm.js",
      "require": "./dist/stencil-initialization-bug/stencil-initialization-bug.cjs.js"
    },
    "./my-component": {
      "import": "./dist/components/my-component.js",
      "types": "./dist/components/my-component.d.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ionic-team/stencil-component-starter.git"
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate"
  },
  "devDependencies": {
    "@stencil/core": "^4.13.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.2",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "puppeteer": "^22.6.1",
    "typescript": "^5.4.3"
  },
  "license": "MIT"
}
